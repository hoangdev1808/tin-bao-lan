section.block-section.tbl-contact.wow.fadeInDown(data-wow-delay=".3s")
	.container
		.block__title.text-uppercase.text-center
			h3 liên hệ
		.tbl-contact__content
			.row
				.col-md-6
					.tbl-contact__content__infor
						.tbl-contact__content__infor__logo
							img(src="./assets/logo.png" alt="")
						.tbl-contact__content__infor__title
							h4 Công ty TNHH Tín Bảo Lan
						.tbl-contact__content__infor__address
							ul
								li
									span
										i.material-icons
											|location_on
									p 160/47 Bùi Đình Túy, Phường 12, Quận Bình Thạnh, TP.HCM
								li
									span
										i.material-icons
											|monetization_on
									p 0304 876 381
								li
									span
										i.material-icons
											|chrome_reader_mode
									p 034001504690001 tại NH TMCP Đông Á- CN Phú Nhuận
								li
									span
										i.material-icons
											|assignment_ind
									p Phạm Văn Tín - Giám Đốc
								li
									span
										i.material-icons
											|call
									p (028) 7300 1399
								li
									span
										i.material-icons
											|print
									p (028) 7300 1399
				.col-md-6
					.box-form
						.form-group
							input(type='text' placeholder='Họ và tên')
						.form-group
							input(type='text' placeholder='Số điện thoại')
						.form-group
							input(type='text' placeholder='Địa chỉ')
						.form-group
							input(type='text' placeholder='Email')
						.form-group.form-title
							input(type='text' placeholder='Tiêu đề')
						.form-group.qparagraph 
							textarea(cols="30", rows="5" placeholder='Nội dung')
						.form-group.frm-captcha
							.frm-captcha-input
								input(type='text' placeholder='Nhập mã hiển thị')
							.RadCaptcha.RadCaptcha_Default
								a.rcRefreshImage(title='Hình ảnh mới' href="") Hình ảnh mới
								img(alt='' src='./assets/images/contact/captcha.png')
						.form-group.frm-btnwrap
							.frm-btn
								input.btn.btn-default.frm-btn-submit(type='submit' name="" value='Gửi')

