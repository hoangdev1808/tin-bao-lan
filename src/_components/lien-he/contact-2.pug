section.block-section.tbl-contact.wow.fadeInDown(data-wow-delay=".3s")
	.container
		.block__title.text-uppercase.text-center
			h3 liên hệ
		.tbl-contact__content
			.row
				.col-md-6
					.tbl-contact__content__infor
						.tbl-contact__content__infor__logo
							img(src="./assets/logo.png", alt="")
						.tbl-contact__content__infor__title
							h4 Công ty TNHH Tín Bảo Lan
						.tbl-contact__content__infor__address
							ul
								li
									span
										i.material-icons
											|location_on
									p 160/47 Bùi Đình Túy, Phường 12, Quận Bình Thạnh, TP.HCM
								li
									span
										i.material-icons
											|monetization_on
									p 0304 876 381
								li
									span
										i.material-icons
											|chrome_reader_mode
									p 034001504690001 tại NH TMCP Đông Á- CN Phú Nhuận
								li
									span
										i.material-icons
											|assignment_ind
									p Phạm Văn Tín - Giám Đốc
								li
									span
										i.material-icons
											|call
									p (028) 7300 1399
								li
									span
										i.material-icons
											|print
									p (028) 7300 1399
				.col-md-6
					.tbl-contact__content__form
						.row
							.col-md-6.form-group
								input(type="text" placeholder="Họ và tên")
							.col-md-6.form-group
								input(type="text" placeholder="Địa chỉ")
							.col-md-6.form-group
								input(type="text" placeholder="Email")
							.col-md-6.form-group
								input(type="text" placeholder="Số điện thoại")
							.col-md-12.form-group
								input(type="text" placeholder="Tiêu đề")
							.col-md-12.form-group
								textarea(name="", cols="30", rows="5" placeholder="Nội dung")
						.form-group.frm-captcha
							.RadCaptcha.RadCaptcha_Default
							.frm-captcha-input
								input#ctl00_mainContent_ctl05_txtCaptcha(name='ctl00$mainContent$ctl05$txtCaptcha', type='text', maxlength='5', placeholder='Nhập mã hiển thị')

							.img-captcha
									img(src="./assets/images/contact/captcha.png", alt="")
								li.refresh
									a(href="")
										span.material-icons replay
								li.input
									input(type="text" placeholder="Nhập mã captcha")
							.block__btn
								a.btn.btn__new(href="") gửi

